import matplotlib.pyplot as plt

lt1 = []
lt2 = []
text = []

data_f = open("DS3.txt")
with data_f as f:
    for line in f:
            text.append([int(x) for x in line.split()])

for i in range(len(text)):
    for j in range(1):
        lt1.append(text[i][0])
        lt2.append(960 - text[i][1])

plt.scatter(lt1, 2)
plt.show()
plt.close()
data_f.close()
